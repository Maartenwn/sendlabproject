		db.test_devices.aggregate([
		  {"$project":{"arrayofkeyvalue":{"$objectToArray":"$$ROOT.testData1"}}},
		  {"$unwind":"$arrayofkeyvalue"},
		  {"$group":{"_id":null,"allkeys":{"$addToSet": "$arrayofkeyvalue.k"}}},
		  {"$project" :{
				"test" : {
					"$filter": {
					 input: "$allkeys",
					 as: "num",
					 cond: { 
						$and: [
							{ "$ne": [ "$$num", "__v"] },
							{ "$ne": [ "$$num", "_id" ] },
							{ "$ne": [ "$$num" , "timestamp"] }
						]
					 }
				  }
				}
			}
		  }
		])	
		
		
		
		